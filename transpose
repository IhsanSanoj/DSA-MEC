#include<stdio.h>
#define max 10

typedef struct{
int row;
int col;
int val;
}term;
void main(){
term a[max],t[max];
int i;
a[0]=(term){3,4,4};
a[1]=(term){0,0,1};
a[2]=(term){0,2,2};
a[3]=(term){1,2,3};
a[4]=(term){2,3,3};
int r=a[0].row;
int c=a[0].col;
int v=a[0].val;
t[0]=(term){c,r,v};
for(int i=1;i<=v;i++){
t[i].row=a[i].col;
t[i].col=a[i].row;
t[i].val=a[i].val;
}
printf("Original matrix\n");
for(i=0;i<=v;i++){
printf("(%d,%d,%d)\n",a[i].row,a[i].col,a[i].val);
}
printf("\n");
printf("transpose matrix\n");
for(i=0;i<=v;i++){
printf("(%d,%d,%d)\n",t[i].row,t[i].col,t[i].val);
}}
